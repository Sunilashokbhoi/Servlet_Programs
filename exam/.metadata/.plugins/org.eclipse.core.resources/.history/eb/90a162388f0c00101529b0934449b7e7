package com.exam;

import java.sql.DriverManager;

public class Driver {
public static void main(String[] args) {
	try{
	Class.forName("oracle.jdbc.driver.OracleDriver");
	Connection con  = DriverManager.getConnection
			("jdbc:oracle:thin:");
	}catch(Exception e) {
		e.printStackTrace();
	}
	}
	
}
